<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.LOCAL_MAC_ADDRESS"
        tools:ignore="ProtectedPermissions" />

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebViewActivity" />
        <activity android:name=".SecondActivity" />

        <activity android:name=".ThirdActivity" />
        <activity android:name=".FirstActivity" />
        <activity android:name=".RepeatActivity" />
        <activity android:name=".TabViewActivity" />
        <activity android:name=".ManualRUMActivity" />
        <activity android:name=".SRActivity" />
        <activity android:name=".HighLoadActivity" />
        <activity android:name=".IgnoreClassActivity" />
        <activity android:name=".SRPrivacyOverrideActivity" />
        <activity android:name=".FragmentActivity" />
        <activity android:name=".ClickTestActivity" />
        <activity android:name=".HandlerTestActivity" />
        <activity android:name=".ActivityTrackingTestActivity" />
        <activity android:name=".FragmentTrackingTestActivity" />

        <service
            android:name=".service.TestService"
            android:process=":test_process" />

        <receiver
            android:name=".directboot.BootBroadcastReceiver"
            android:directBootAware="true"
            android:exported="false"
            android:process=":another_process">
            <!-- Listening the BOOT_COMPLETED action for legacy pre-N devices -->
            <intent-filter>
                <!--                <action android:name="android.intent.action.LOCKED_BOOT_COMPLETED" />-->
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

    </application>

</manifest>